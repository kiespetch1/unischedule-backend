{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ApiDocumentationSettings": {
    "Swagger": {
      "Version": "v1",
      "Title": "R.Tech Schedule API",
      "DocumentTitle": "Schedule API",
      "Description": "Сервис управления расписанием",
      "RouteTemplate": "/internal/api/{documentname}/swagger.json",
      "RoutePrefix": "internal/api"
    },
    "Redoc": {
      "SpecUri": "/internal/api/v1/swagger.json",
      "DocumentTitle": "Schedule API Docs",
      "RoutePrefix": "internal/api/docs"
    }
  },
  "AllowedHosts": "*"
}
